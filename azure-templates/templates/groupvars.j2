--- 
{% if os_name | regex_search("windows", ignorecase=True) %}ansible_user: {{ os_adminuser }}
ansible_connection: winrm 
ansible_winrm_server_cert_validation: ignore 
ansible_port: 5986
ansible_winrm_connection_timeout: 1800
ansible_winrm_operation_timeout_sec: 1800
ansible_winrm_read_timeout_sec: 1800
#ansible_winrm_transport: ntlm
{% else %} 
ansible_user: {{ os_adminuser }}
ansible_connection: ssh
ansible_port: 22
ansible_host_key_checking: no
{% endif %}
