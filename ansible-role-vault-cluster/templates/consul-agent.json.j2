{
  "server": false,
  "datacenter": "{{ consul_dc }}",
  "node_name": "{{ vault_nodename }}",
  "data_dir": "/var/consul/data",
  "bind_addr": "{{ (vault_nodes | selectattr('name','equalto', vault_nodename) | first).ip }}",
  "client_addr": "127.0.0.1",
  "retry_join": ["{{ consul_nodes | map(attribute='ip') | list | join('\",\"') }}"],
  "log_level": "DEBUG",
  "enable_syslog": true,
  "acl_enforce_version_8": false
}
